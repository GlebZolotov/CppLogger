    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
                // {
                //     "description": "Pagination off for independent threads",
                //     "text": "-gdb-set pagination off",
                //     "ignoreFailures": true
                // },
                // {
                //     "description":  "Independent threads",
                //     "text": "-gdb-set non-stop on",
                //     "ignoreFailures": true
                // }
            ]
        }

    ]
}